<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".Driverss_performanceActivity"
    tools:openDrawer="start">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/accln"
            android:layout_width="372dp"
            android:layout_height="55dp"
            android:backgroundTint="#FDDE82"
            android:fontFamily="@font/nmu"
            android:padding="10dp"
            android:paddingTop="20dp"
            android:text="Drivers Performance"
            android:textColor="#FFFF"
            android:textColorHint="#FFFFFF"
            android:textSize="28sp"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.cardview.widget.CardView
            android:id="@+id/NewList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="29dp"
            android:layout_marginRight="15dp"
            app:cardBackgroundColor="#0e4357"
            app:cardCornerRadius="20dp"
            app:cardElevation="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/accln">

            <LinearLayout
                android:layout_width="356dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/accln_txt1"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/nmr"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text="Accln -:"
                    android:textColor="#FFFF"
                    android:textColorHint="#FFFFFF"
                    android:textSize="28sp" />

                <TextView
                    android:id="@+id/accln_tx"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/fredoka_one"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text=""
                    android:textColor="#FFFF"
                    android:textColorHint="#FFFFFF"
                    android:textSize="28sp" />


            </LinearLayout>
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/NewList12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="29dp"
            android:layout_marginRight="15dp"
            app:cardBackgroundColor="#0e4357"
            app:cardCornerRadius="20dp"
            app:cardElevation="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/NewList">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/speed_txt1"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/nmr"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text="Speed -:"
                    android:textColor="#FFFF"
                    android:textColorHint="#FFFFFF"
                    android:textSize="28sp" />
                <TextView
                    android:id="@+id/speed"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/fredoka_one"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text=""
                    android:textColor="#FFFF"
                    android:textColorHint="#FFFFFF"
                    android:textSize="28sp" />


            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/ActiveLists"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="29dp"
            android:layout_marginRight="15dp"
            app:cardBackgroundColor="#0e4357"
            app:cardCornerRadius="20dp"
            app:cardElevation="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/NewList12">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/harshBreak"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/nmr"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text="Harsh Break - "
                    android:textColor="#FFFF"
                    android:textSize="28sp" />
                <TextView
                    android:id="@+id/harshBreakTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/nmr"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text="5"
                    android:textColor="#FFFF"
                    android:textSize="28sp" />


            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/overspeed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="29dp"
            android:layout_marginRight="15dp"
            app:cardBackgroundColor="#0e4357"
            app:cardCornerRadius="20dp"
            app:cardElevation="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ActiveLists">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/overspeeding"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/nmr"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text="OverSpeed - 3"
                    android:textColor="#FFFF"
                    android:textSize="28sp" />


            </LinearLayout>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/score"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="29dp"
            android:layout_marginRight="15dp"
            app:cardBackgroundColor="#0e4357"
            app:cardCornerRadius="20dp"
            app:cardElevation="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/overspeed">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Scoretxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/nmr"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text="Score   -"
                    android:textColor="#FFFF"
                    android:textSize="28sp" />
                <TextView
                    android:id="@+id/scoreval"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="31dp"
                    android:backgroundTint="#FDDE82"
                    android:fontFamily="@font/nmr"
                    android:padding="10dp"
                    android:paddingTop="20dp"
                    android:text=" 75"
                    android:textColor="#FFFF"
                    android:textSize="28sp" />


            </LinearLayout>

        </androidx.cardview.widget.CardView>




        <!--        <com.airbnb.lottie.LottieAnimationView-->
