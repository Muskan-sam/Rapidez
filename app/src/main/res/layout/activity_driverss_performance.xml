        <!--            app:lottie_loop="true"-->
        <!--            app:lottie_rawRes="@raw/bottom"-->
        <!--            app:lottie_speed="2" />-->

    </androidx.constraintlayout.widget.ConstraintLayout>





</androidx.drawerlayout.widget.DrawerLayout>